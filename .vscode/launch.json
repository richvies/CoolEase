{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch cortex debug",
      "type": "cortex-debug",
      "armToolchainPath": "/home/rick/.local/share/mise/installs/gcc-arm-none-eabi/14.2.Rel1/bin",
      "cpu": "cortex-m3",
      "executable": "build/Debug/hub/hub_Debug.elf",
      "runToEntryPoint": "main",
      "request": "launch",
      "gdbPath": "/home/rick/.local/share/mise/installs/gcc-arm-none-eabi/14.2.Rel1/bin/arm-none-eabi-gdb",
      "svdFile": "Tools/OpenOCD/STM32L052.svd",
      "stlinkPath": "/usr/bin/stlink-server",
      "showDevDebugOutput": "raw",
      "servertype": "stutil"
    },
    {
      "name": "Hub debug",
      "type": "cortex-debug",
      "request": "launch",
      "servertype": "openocd",
      "cwd": "${workspaceFolder}",
      "svdFile": "Tools/OpenOCD/STM32L052.svd",
      "configFiles": [
        "Tools/OpenOCD/stlink.cfg",
        "Tools/OpenOCD/stm32l0.cfg"
      ],
      "preLaunchTask": "Build hub",
      "executable": "hub/bin/hub.elf"
    },
    {
      "name": "Hub bootloader debug",
      "type": "cortex-debug",
      "request": "launch",
      "servertype": "openocd",
      "cwd": "${workspaceFolder}",
      "svdFile": "Tools/OpenOCD/STM32L052.svd",
      "configFiles": [
        "Tools/OpenOCD/stlink.cfg",
        "Tools/OpenOCD/stm32l0.cfg"
      ],
      "preLaunchTask": "Build hub",
      "executable": "hub/bin/hub_bootloader.elf",
      "gdbPath": "/usr/local/bin/arm-none-eabi-gdb"
    },
    {
      "name": "Sensor debug",
      "type": "cortex-debug",
      "request": "launch",
      "servertype": "openocd",
      "cwd": "${workspaceFolder}",
      "svdFile": "Tools/OpenOCD/STM32L052.svd",
      "configFiles": [
        "Tools/OpenOCD/stlink.cfg",
        "Tools/OpenOCD/stm32l0.cfg"
      ],
      "preLaunchTask": "Build sensor",
      "executable": "sensor/bin/sensor.elf"
    },
    {
      "name": "Sensor bootloader debug",
      "type": "cortex-debug",
      "request": "launch",
      "servertype": "openocd",
      "cwd": "${workspaceFolder}",
      "svdFile": "Tools/OpenOCD/STM32L052.svd",
      "configFiles": [
        "Tools/OpenOCD/stlink.cfg",
        "Tools/OpenOCD/stm32l0.cfg"
      ],
      "preLaunchTask": "Build sensor",
      "executable": "sensor/bin/sensor_bootloader.elf"
    },
    {
      "name": "Debug current Python file",
      "type": "python",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "program": "${file}",
      "console": "integratedTerminal"
    }
  ]
}